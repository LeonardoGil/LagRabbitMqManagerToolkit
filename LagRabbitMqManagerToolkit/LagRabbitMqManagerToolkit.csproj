<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<PackageId>LagRabbitMqManagerToolkit</PackageId>
		<Version>1.0.0-alpha</Version>
		<Authors>Leonardo Albuquerque Gil</Authors>
		<Company>Lag</Company>
		<Description>Biblioteca em .NET para consumo da API do RabbitMQ Manager.</Description>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<PackageProjectUrl>https://github.com/LeonardoGil/LagRabbitMqManagerToolkit</PackageProjectUrl>
		<RepositoryUrl>https://github.com/LeonardoGil/LagRabbitMqManagerToolkit</RepositoryUrl>

		<TargetFramework>net8.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="9.0.4" />
		<PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
	</ItemGroup>

</Project>
